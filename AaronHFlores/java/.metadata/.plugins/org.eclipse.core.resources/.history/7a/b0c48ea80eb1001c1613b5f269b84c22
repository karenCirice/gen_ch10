package java01;

public class HolaMundo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Hola Mundo desde el IDE");
		//Definimos una variable boolean
		boolean result = true;
		System.out.println(result);
		//Definimos una variable de tipo char
		char capitalC = 'C';
		System.out.println(capitalC);
		//Definimos una variable de tipo byte
		byte b = 100;
		System.out.println(b);
		//Definimos una variable de tipo shart
		short s = 10000;
		System.out.println(s);
		//Definimos una variable de tipo int
		int i = 1000000;
		System.out.println(i);
		//Definimos una variable de tipo int en hexa
		int hexVal = 0x1a;
		System.out.println(hexVal);
		//Definimos una variable de tipo int en binario
		int binVal = ob11010;
		System.out.println(binVal);
		//Definimos una variable de tipo int en binario
		int octVal = ob11010;
		System.out.println(octVal);
	}

}
