public class Main {

	public static void main(String[] args) {
		boolean condicion = false;
		//If se ejecuta si la condición es verdadera
		if(condicion) {
			System.out.println("La condición es verdadera");
		}
		System.out.println("Se continua con otras instrucciones");
	}

}
